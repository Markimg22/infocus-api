{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": [
		"standard"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
    "no-undef": "off",
    "space-before-function-paren": "off",
    "no-useless-constructor": "off",
    "no-use-before-define": "off",
    "no-redeclare": "off",
    "import/export": "off"
  }
}
